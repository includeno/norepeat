{
   "background": {
      "scripts": [
         "background.js"
      ],
      "persistent":true
   },
   "icons": {
      "48": "icon.png"
   },
   "content_scripts": [
      {
         "js": [
            "jquery-3.5.1.min.js","content.js"
         ],
         "css": [],
         "matches": [
            "http://*/*",
            "https://*/*"
         ],
         "run_at": "document_start"
      }
   ],
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "manifest_version": 2,
   "name": "Personal Search Assistant",
   "permissions": [
      "tabs",
      "notifications",
      "contextMenus",
      "webRequest",
      "webRequestBlocking",
      "unlimitedStorage",
      "storage",
      "gcm"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1",
   "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval' https://cdn.bootcss.com https://unpkg.com https://cdn.bootcdn.net; object-src 'self' ;",

   "web_accessible_resources":[
      "js/*","*.jepg"
   ]
}